{
    "name": "pgs/hashid-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Hash Id",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Karol Jonski",
            "email": "kjonski@pgs-soft.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/dependency-injection": "^3.4.3|^4.0.3",
        "symfony/config": "~3.4|~4.0"
    },
    "autoload": {
        "psr-4": { "Pgs\\HashIdBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pgs\\HashIdBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
      "branch-alias": {
        "dev-master": "master"
      }
    }
}
